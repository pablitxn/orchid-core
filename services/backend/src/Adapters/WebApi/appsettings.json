{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.SignalR": "Debug",
      "Microsoft.AspNetCore.Http.Connections": "Debug",
      "Microsoft.AspNetCore.SignalR.HubMethodInvocationDispatcher": "Debug",
      "Microsoft.AspNetCore.SignalR.HubEndPoint": "Debug"
    }
  },
  "FrontendUrl": "http://localhost:5173",
  "AllowedHosts": "*",
  "Authorization": {
    "Google": {
      "UserInfoUrl": "https://www.googleapis.com/oauth2/v1/userinfo"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5433;Database=orchid_core;Username=admin;Password=admin"
  },
  "OpenAI": {
    "Endpoint": "https://api.openai.com/v1/chat/completions",
    "ApiKey": "",
    "Model": "gpt-4o"
  },
  "Langfuse": {
    "BaseUrl": "http://localhost:3033/api/public",
    "Enabled": true,
    "PublicKey": "",
    "SecretKey": ""
  },
  "Redis": {
    "Configuration": "localhost:6379,password=1234",
    "InstanceName": "ChatHistory:"
  },
  "FileStorage": {
    "Provider": "Local",
    "Bucket": "playground-bucket",
    "Path": "data"
  },
  "Auth": {
    "Provider": "generic"
  },
  "Jwt": {
    "Key": "dev-secret-key",
    "Issuer": "playground",
    "Audience": "playground",
    "ExpireHours": 72
  },
  "TableDetection": {
    "MaxRetries": 3,
    "InputTokenCostPer1K": 0.01,
    "OutputTokenCostPer1K": 0.03,
    "MaxContextLength": 128000
  },
  "CreditSystem": {
    "TokensPerCredit": 100,
    "MinimumCreditsPerMessage": 1
  }
}
